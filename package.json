{
  "name": "shiny-guacamole",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "audit:report": "npm audit --json > audit-report.json && npm audit --audit-level=moderate",
    "audit:high": "npm audit --audit-level=high",
    "audit:critical": "npm audit --audit-level=critical",
    "security:check": "npm audit && npm outdated",
    "security:fix": "npm audit fix --force",
    "security:update": "npm update",
    "deps:check": "npm outdated",
    "deps:update": "npm update",
    "deps:upgrade": "npx npm-check-updates -u",
    "vulnerability:scan": "npm audit --json | jq '.vulnerabilities'",
    "security:full": "npm audit && npm outdated && echo 'Security scan completed. Check audit-report.json for details.'"
  },
  "dependencies": {
    "react": "^18",
    "react-dom": "^18",
    "next": "14.2.13",
    "@supabase/supabase-js": "^2.39.7",
    "@supabase/auth-helpers-nextjs": "^0.8.7",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8"
  },
  "devDependencies": {
    "typescript": "^5",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "tailwindcss": "^3.4.1",
    "autoprefixer": "^10.0.1",
    "postcss": "^8",
    "eslint": "^8",
    "eslint-config-next": "14.2.13"
  }
}
